{
  "name": "larryli/ipv4",
  "description": "IP v4 中国城市地址库",
  "homepage": "https://github.com/larryli/ipv4",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Larry Li",
      "email": "larryli@qq.com"
    }
  ],
  "require": {
    "php": ">=5.4",
    "symfony/console": "~2.5",
    "catfan/medoo": "~0.9.8"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.5",
    "yiisoft/yii2": ">=2.0.6",
    "yiisoft/yii2-bootstrap": "*",
    "yiisoft/yii2-swiftmailer": "*",
    "yiisoft/yii2-gii": "*",
    "yiisoft/yii2-debug": "*"
  },
  "suggest": {
    "ext-pdo-mysql": "For MySQL or MariaDB databases",
    "ext-pdo-sqlite": "For SQLite databases",
    "yiisoft/yii2": ">=2.0.6"
  },
  "autoload": {
    "psr-4": { "larryli\\ipv4\\": "src/" }
  },
  "bin": ["bin/ipv4"],
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
