#!/usr/bin/env php
<?php

if (PHP_SAPI !== 'cli') {
    echo 'Warning: Composer should be invoked via the CLI version of PHP, not the '.PHP_SAPI.' SAPI'.PHP_EOL;
}
require __DIR__.'/../src/bootstrap.php';

use Symfony\Component\Console\Application;

$app = new Application('IP v4', '1.0-dev');
$app->add(new larryli\ipv4\commands\InitCommand);
$app->add(new larryli\ipv4\commands\QueryCommand);
$app->add(new larryli\ipv4\commands\CleanCommand);
$app->add(new larryli\ipv4\commands\DumpCommand);
$app->add(new larryli\ipv4\commands\BenchmarkCommand);
$app->run();
