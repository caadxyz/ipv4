#!/usr/bin/env php
<?php

if (PHP_SAPI !== 'cli') {
    echo 'Warning: Composer should be invoked via the CLI version of PHP, not the '.PHP_SAPI.' SAPI'.PHP_EOL;
}
require __DIR__.'/../src/bootstrap.php';

use Symfony\Component\Console\Application;

$app = new Application('IP v4', '1.0');
$app->add(new larryli\IPv4\Command\InitCommand);
$app->add(new larryli\IPv4\Command\DumpCommand);
$app->add(new larryli\IPv4\Command\QueryCommand);
//$app->add(new larryli\IPv4\Command\TestCommand);
$app->run();
